        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_1000">
          <gml:description>Festsetzung in Bebauungsplan.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:1000</gml:identifier>
