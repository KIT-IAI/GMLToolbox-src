          <gml:name>NachrichtlicheUebernahme</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_3000">
          <gml:description>Hinweis nach BauGB</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:3000</gml:identifier>
          <gml:name>Hinweis</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_4000">
