<TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ../xsd/ShapeChangeDefinitions.xsd" gml:id="xplan_BP_Objekt">
  <gml:description>Basisklasse für alle raumbezogenen Festsetzungen,  Hinweise, Vermerke und Kennzeichnungen eines Bebauungsplans.</gml:description>
  <gml:identifier codeSpace="urn:xplan">urn:xplan:element:xplan:BP_Objekt</gml:identifier>
  <gml:name>xplan:BP_Objekt</gml:name>
  <gml:dictionaryEntry>
    <def:PropertyDefinition gml:id="xplan_BP_Objekt_rechtscharakter">
      <gml:description>Rechtliche Charakterisierung des Planinhaltes.</gml:description>
      <gml:identifier codeSpace="urn:xplan">urn:xplan:element:xplan:BP_Objekt:attribute:rechtscharakter</gml:identifier>
      <gml:name>xplan:rechtscharakter</gml:name>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_1000">
          <gml:description>Festsetzung in Bebauungsplan.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:1000</gml:identifier>
          <gml:name>Festsetzung</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_2000">
          <gml:description>Nachrichtliche Übernahme aus anderen Planwerken.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:2000</gml:identifier>
          <gml:name>NachrichtlicheUebernahme</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_3000">
          <gml:description>Hinweis nach BauGB</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:3000</gml:identifier>
          <gml:name>Hinweis</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_4000">
          <gml:description>Vermerk nach § 5 BauGB</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:4000</gml:identifier>
          <gml:name>Vermerk</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_5000">
          <gml:description>Kennzeichnung von Flächen nach $9 Absatz 5 BauGB. Kennzeichnungen sind keine rechtsverbindlichen Festsetzungen, sondern Hinweise auf Besonderheiten (insbesondere der Baugrundverhältnisse), deren Kenntnis für das Verständnis des Bebauungsplans und seiner Festsetzungen wie auch für die Vorbereitung und Genehmigung von Vorhaben notwendig sind.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:5000</gml:identifier>
          <gml:name>Kennzeichnung</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_BP_Objekt_rechtscharakter_BP_Rechtscharakter_9998">
          <gml:description>Der Rechtscharakter des BPlan-Inhaltes ist unbekannt.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:BP_Objekt:rechtscharakter:BP_Rechtscharakter:9998</gml:identifier>
          <gml:name>Unbekannt</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <def:cardinality>1</def:cardinality>
      <def:valueTypeName>BP_Rechtscharakter</def:valueTypeName>
      <def:type>attribute</def:type>
    </def:PropertyDefinition>
