        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_RP_Forstwirtschaft_typ_RP_ForstwirtschaftTypen_3001">
          <gml:description>Besondere Schutzfunktion des Waldes.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:RP_Forstwirtschaft:typ:RP_ForstwirtschaftTypen:3001</gml:identifier>
          <gml:name>BesondereSchutzfunktionDesWaldes</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_RP_Forstwirtschaft_typ_RP_ForstwirtschaftTypen_4000">
          <gml:description>Von Aufforstung freizuhaltendes Gebiet.</gml:description>
