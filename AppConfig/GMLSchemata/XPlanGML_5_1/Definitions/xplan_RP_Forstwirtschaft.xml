        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_RP_Forstwirtschaft_typ_RP_ForstwirtschaftTypen_9999">
          <gml:description>Sonstige Forstwirtschaft.</gml:description>
          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:RP_Forstwirtschaft:typ:RP_ForstwirtschaftTypen:9999</gml:identifier>
          <gml:name>SonstigeForstwirtschaft</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <def:cardinality>0..1</def:cardinality>
      <def:valueTypeName>RP_ForstwirtschaftTypen</def:valueTypeName>
