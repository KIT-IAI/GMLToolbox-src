      <documentation>Aufzählung der Planungszustände eines Planinhalts.
</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="1000">
        <!--Geplant-->
      </enumeration>
      <enumeration value="2000">
        <!--Bestehend-->
      </enumeration>
      <enumeration value="3000">
        <!--Fortfallend-->
      </enumeration>
    </restriction>
  </simpleType>
  <!--=========================================================================================-->
  <!--XP_SPEMassnahmenDaten-->
