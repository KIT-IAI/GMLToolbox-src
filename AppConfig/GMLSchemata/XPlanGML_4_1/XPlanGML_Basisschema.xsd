    </sequence>
  </complexType>
  <element name="_GenericApplicationPropertyOfXP_Plangeber" type="anyType" abstract="true" />
  <complexType name="XP_PlangeberPropertyType">
    <sequence>
      <element ref="xplan:XP_Plangeber" minOccurs="0" />
    </sequence>
  </complexType>
