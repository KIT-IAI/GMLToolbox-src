  <simpleType name="LP_Rechtsstand">
    <restriction base="string">
      <enumeration value="1000">
        <!--Aufstellungsbeschluss-->
      </enumeration>
      <enumeration value="2000">
        <!--Entwurf-->
      </enumeration>
      <enumeration value="3000">
        <!--Plan-->
      </enumeration>
      <enumeration value="4000">
        <!--Wirksamkeit-->
      </enumeration>
      <enumeration value="5000">
        <!--Untergegangen-->
      </enumeration>
    </restriction>
  </simpleType>
  <!--=========================================================================================-->
  <!--LP_Status-->
  <!--=========================================================================================-->
  <simpleType name="LP_Status">
    <restriction base="string">
      <enumeration value="1000">
        <!--Festsetzung-->
      </enumeration>
      <enumeration value="2000">
        <!--Geplant-->
      </enumeration>
      <enumeration value="3000">
        <!--NachrichtlichUebernommen-->
      </enumeration>
      <enumeration value="4000">
        <!--DarstellungKennzeichnung-->
