          <element name="weitereFunktion1" minOccurs="0" type="xplan:LP_ErholungFreizeitFunktionen" />
          <element name="weitereFunktion2" minOccurs="0" type="xplan:LP_ErholungFreizeitFunktionen" />
          <element name="detaillierteFunktion" minOccurs="0" maxOccurs="unbounded" type="gml:CodeType">
