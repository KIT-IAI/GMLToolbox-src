			<xs:element name="legendeURL" type="xs:anyURI" minOccurs="0"/>
			<xs:element name="textURL" type="xs:anyURI" minOccurs="0"/>
		</xs:sequence>
		</xs:extension>
		</xs:complexContent>
	</xs:complexType>

