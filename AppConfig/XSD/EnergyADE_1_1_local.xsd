 <!--=========================================================================================-->
 <!--HeatSourceTypeValue-->
 <!--=========================================================================================-->
 <simpleType name="HeatSourceTypeValue">
  <annotation>
   <documentation>Enumeration of different types of heat sources for a heat pump</documentation>
  </annotation>
  <restriction base="string">
   <enumeration value="ambientAir">
    <annotation>
     <documentation>Heat is available for free from environment sources like the Sun for heating
      water, building material, or air.</documentation>
    </annotation>
   </enumeration>
   <enumeration value="aquifer">
    <annotation>
     <documentation>It is an underground layer of water bearing permeable rock from which ground
      water can be extracted using a water well.</documentation>
    </annotation>
   </enumeration>
   <enumeration value="exhaustAir">
    <annotation>
     <documentation>Extracts heat from the exhaust air of a building and transfer the heat to the
      supply air, hot tap water and/or hydronic heating system (underfloor heating,
      radiators).</documentation>
    </annotation>
   </enumeration>
   <enumeration value="horizontalGroundCollector">
    <annotation>
     <documentation>A central heating and/or cooling system that transfers heat to or from the
      ground (installed horizontally).</documentation>
    </annotation>
   </enumeration>
   <enumeration value="verticalGroundCollector">
    <annotation>
     <documentation>A central heating and/or cooling system that transfers heat to or from the
      ground (installed vertically).</documentation>
    </annotation>
   </enumeration>
  </restriction>
 </simpleType>
