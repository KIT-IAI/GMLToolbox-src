 <!--MediumTypeValue-->
 <!--=========================================================================================-->
 <simpleType name="MediumTypeValue">
  <annotation>
   <documentation>Enumeration for classifying the medium of a thermal energy distribution
    system.</documentation>
  </annotation>
  <restriction base="string">
   <enumeration value="air">
    <annotation>
     <documentation>The invisible gaseous substance surrounding the earth, a mixture mainly of
      oxygen and nitrogen.</documentation>
    </annotation>
   </enumeration>
   <enumeration value="steam">
    <annotation>
     <documentation>The vapour into which water is converted when heated.</documentation>
    </annotation>
   </enumeration>
   <enumeration value="water">
    <annotation>
     <documentation>A colourless, transparent, odourless, liquid which forms the seas, lakes,
      rivers, and rain and is the basis of the fluids of living organisms.</documentation>
    </annotation>
   </enumeration>
  </restriction>
 </simpleType>
